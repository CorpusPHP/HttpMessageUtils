{
	"name": "corpus/http-message-utils",
	"description": "Utilities for working with Psr7 Http Messages",
	"type": "library",
	"require": {
		"psr/http-message": "^1.1",
		"php": ">=7.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5 || ^9.3",
		"php-mock/php-mock-phpunit": "^2.7",
		"guzzlehttp/psr7": "^1.9",
		"corpus/coding-standard": "^0.6.0",
		"squizlabs/php_codesniffer": "^3.7",
		"friendsofphp/php-cs-fixer": "^3.3",
		"donatj/mddoc": "dev-master",
		"donatj/drop": "^1.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Jesse Donat",
			"email": "donatj@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Corpus\\HttpMessageUtils\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
